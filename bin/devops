#!/bin/bash

source ./log.sh
source ./tools.sh
source ./env.sh
source ./build.sh

if test ! -d $HOME/.devops ; then mkdir $HOME/.devops ; fi

#设置deploy文件生成位置
info "埋点: 当前的工作空间为:${env[opt_workspace]}"


#执行first命令
case ${env[cmd_1]} in
        run) run; ;;
        *) echo "cannot find the cammand ${env[cmd_1]}"; exit1 ; ;;
esac

